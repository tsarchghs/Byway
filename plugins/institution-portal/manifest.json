{
  "name": "institutions-portal",
  "routes": [
    {
      "path": "/institution/student_mode/[institution_id]",
      "file": "nuxt/pages/student_mode_institution_dashboard.vue"
    },
    {
      "path": "/institution/[institution_id]/backup",
      "file": "nuxt/pages/inst-portal-role-based.vue"
    },
    {
      "path": "/institutions-portal",
      "file": "nuxt/pages/role-portal/index.vue"
    }
  ],
  "server": {
    "index": "server/index.mjs",
    "routes": [
      {
        "method": "GET",
        "path": "/api/institution-portal/overview"
      },
      {
        "method": "GET",
        "path": "/api/institution-portal/student-dashboard"
      }
    ]
  }
}
