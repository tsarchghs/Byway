#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/gjergjkadriu/Documents/GitHub/Byway/node_modules/.pnpm/vite@7.1.12_@types+node@22.18.13_jiti@2.6.1_terser@5.44.0_tsx@4.20.6_yaml@2.8.1/node_modules/vite/bin/node_modules:/Users/gjergjkadriu/Documents/GitHub/Byway/node_modules/.pnpm/vite@7.1.12_@types+node@22.18.13_jiti@2.6.1_terser@5.44.0_tsx@4.20.6_yaml@2.8.1/node_modules/vite/node_modules:/Users/gjergjkadriu/Documents/GitHub/Byway/node_modules/.pnpm/vite@7.1.12_@types+node@22.18.13_jiti@2.6.1_terser@5.44.0_tsx@4.20.6_yaml@2.8.1/node_modules:/Users/gjergjkadriu/Documents/GitHub/Byway/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/gjergjkadriu/Documents/GitHub/Byway/node_modules/.pnpm/vite@7.1.12_@types+node@22.18.13_jiti@2.6.1_terser@5.44.0_tsx@4.20.6_yaml@2.8.1/node_modules/vite/bin/node_modules:/Users/gjergjkadriu/Documents/GitHub/Byway/node_modules/.pnpm/vite@7.1.12_@types+node@22.18.13_jiti@2.6.1_terser@5.44.0_tsx@4.20.6_yaml@2.8.1/node_modules/vite/node_modules:/Users/gjergjkadriu/Documents/GitHub/Byway/node_modules/.pnpm/vite@7.1.12_@types+node@22.18.13_jiti@2.6.1_terser@5.44.0_tsx@4.20.6_yaml@2.8.1/node_modules:/Users/gjergjkadriu/Documents/GitHub/Byway/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/vite@7.1.12_@types+node@22.18.13_jiti@2.6.1_terser@5.44.0_tsx@4.20.6_yaml@2.8.1/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/vite@7.1.12_@types+node@22.18.13_jiti@2.6.1_terser@5.44.0_tsx@4.20.6_yaml@2.8.1/node_modules/vite/bin/vite.js" "$@"
fi
