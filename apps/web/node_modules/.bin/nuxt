#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/gjergjkadriu/Documents/GitHub/Byway/node_modules/.pnpm/nuxt@4.2.0_@parcel+watcher@2.5.1_@types+node@22.18.13_@vue+compiler-sfc@3.5.22_db0@0.3.4_iore_osugdioqrxz5r6rprm5dacpjmy/node_modules/nuxt/bin/node_modules:/Users/gjergjkadriu/Documents/GitHub/Byway/node_modules/.pnpm/nuxt@4.2.0_@parcel+watcher@2.5.1_@types+node@22.18.13_@vue+compiler-sfc@3.5.22_db0@0.3.4_iore_osugdioqrxz5r6rprm5dacpjmy/node_modules/nuxt/node_modules:/Users/gjergjkadriu/Documents/GitHub/Byway/node_modules/.pnpm/nuxt@4.2.0_@parcel+watcher@2.5.1_@types+node@22.18.13_@vue+compiler-sfc@3.5.22_db0@0.3.4_iore_osugdioqrxz5r6rprm5dacpjmy/node_modules:/Users/gjergjkadriu/Documents/GitHub/Byway/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/gjergjkadriu/Documents/GitHub/Byway/node_modules/.pnpm/nuxt@4.2.0_@parcel+watcher@2.5.1_@types+node@22.18.13_@vue+compiler-sfc@3.5.22_db0@0.3.4_iore_osugdioqrxz5r6rprm5dacpjmy/node_modules/nuxt/bin/node_modules:/Users/gjergjkadriu/Documents/GitHub/Byway/node_modules/.pnpm/nuxt@4.2.0_@parcel+watcher@2.5.1_@types+node@22.18.13_@vue+compiler-sfc@3.5.22_db0@0.3.4_iore_osugdioqrxz5r6rprm5dacpjmy/node_modules/nuxt/node_modules:/Users/gjergjkadriu/Documents/GitHub/Byway/node_modules/.pnpm/nuxt@4.2.0_@parcel+watcher@2.5.1_@types+node@22.18.13_@vue+compiler-sfc@3.5.22_db0@0.3.4_iore_osugdioqrxz5r6rprm5dacpjmy/node_modules:/Users/gjergjkadriu/Documents/GitHub/Byway/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nuxt/bin/nuxt.mjs" "$@"
else
  exec node  "$basedir/../nuxt/bin/nuxt.mjs" "$@"
fi
