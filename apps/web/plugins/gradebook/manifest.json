{
  "name": "gradebook",
  "displayName": "Gradebook",
  "description": "Shared gradebook service for students and teachers across institutions and courses.",
  "version": "0.1.0",
  "routes": [
    {
      "path": "/gradebook",
      "file": "nuxt/pages/index.vue",
