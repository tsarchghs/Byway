{
  "name": "bloggrs-pluggable",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*",
    "plugins/*"
  ],
  "scripts": {
    "db:generate": "find plugins -name schema.prisma -exec npx prisma generate --schema={} \\;",
    "dev:web": "pnpm --filter @bloggrs/web dev",
    "dev:api": "pnpm --filter @bloggrs/api dev",
    "dev": "concurrently -k -n web,api -c blue,green \"pnpm dev:web\" \"pnpm dev:api\"",
    "build": "pnpm -r build",
    "start:api": "pnpm --filter @bloggrs/api start",
    "migrate:plugins": "node scripts/migrate-plugins.mjs"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@graphql-tools/schema": "^10.0.27",
    "@nuxt/kit": "^4.2.0",
    "@prisma/client": "5.15.0",
    "@vue/apollo-composable": "4.2.2",
    "ant-design-vue": "^4.2.6",
    "apollo-server-express": "^3.13.0",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "chalk": "^5.6.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dataloader": "^2.2.3",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-graphql": "^0.12.0",
    "graphql": "^16.12.0",
    "graphql-request": "^7.3.1",
    "graphql-scalars": "^1.25.0",
    "graphql-tag": "^2.12.6",
    "graphql-type-json": "^0.3.2",
    "graphql-yoga": "^5.16.2",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "nexus": "^1.3.0",
    "node-fetch": "^3.3.2",
    "ofetch": "^1.5.1",
    "ohmyfetch": "^0.4.21",
    "stripe": "^19.3.0",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/node": "^22.18.13",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vue/babel-plugin-jsx": "^2.0.1",
    "@vue/runtime-dom": "^3.5.22",
    "add": "^2.0.6",
    "canvas": "^3.2.0",
    "concurrently": "^9.0.0",
    "gifencoder": "^2.0.1",
    "playwright": "^1.56.1",
    "pnpm": "^10.20.0",
    "prisma": "5.15.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.2",
    "vite": "^7.1.12",
    "vue-tsc": "^3.1.3",
    "vue-tsx-support": "^3.2.0"
  },
  "packageManager": "pnpm@9.11.0+sha512.0a203ffaed5a3f63242cd064c8fb5892366c103e328079318f78062f24ea8c9d50bc6a47aa3567cabefd824d170e78fa2745ed1f16b132e16436146b7688f19b"
}
